{
  "name": "iita-akilimo/ana-data-plaform",
  "description": "This is a simple platform to allow data sharing in the form of file downloads",
  "authors": [
    {
      "name": "Sammy Barasa",
      "email": "barsamms@gmail.com",
      "role": "Developer"
    }
  ],
  "keywords": [
    "yii2",
    "framework",
    "AKILIMO",
    "ona"
  ],
  "homepage": "https://ana.akilimo.org",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "https://www.yiiframework.com/forum/",
    "wiki": "https://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1",
    "yiisoft/yii2": "~2.0.45",
    "yiisoft/yii2-bootstrap5": "^2.0.",
    "yiisoft/yii2-symfonymailer": "~2.0.3",
    "twbs/bootstrap-icons": "^1.10",
    "yii2mod/yii2-user": "^2.0",
    "kartik-v/yii2-bootstrap5-dropdown": "*",
    "kartik-v/yii2-grid": "dev-master",
    "masgeek/filemanager": "dev-main"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.2.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "phpunit/phpunit": "~9.5.0",
    "codeception/codeception": "^5.0.0 || ^4.0",
    "codeception/lib-innerbrowser": "^4.0 || ^3.0 || ^1.1",
    "codeception/module-asserts": "^3.0 || ^1.1",
    "codeception/module-yii2": "^1.1",
    "codeception/module-filesystem": "^3.0 || ^2.0 || ^1.1",
    "codeception/verify": "^3.0 || ^2.2",
    "symfony/browser-kit": "^6.0 || >=2.7 <=4.2.4",
    "mootensai/yii2-enhanced-gii": "^1.0"
  },
  "replaced": {
    "bower-asset/jquery": ">=1.11.0",
    "bower-asset/inputmask": ">=3.2.0",
    "bower-asset/punycode": ">=1.3.0",
    "bower-asset/yii2-pjax": ">=2.0.0",
    "bower-asset/bootstrap": ">=5.0.0",
    "bower-asset/fontawesome": ">=4.0.0"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    },
    "process-timeout": 1800,
    "apcu-autoloader": true,
    "fxp-asset": {
      "enabled": false
    }
  },
  "scripts": {
    "post-install-cmd": [
      "yii\\composer\\Installer::postInstall"
    ],
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject",
      "yii\\composer\\Installer::postInstall"
    ],
    "post-update-cmd": [
      "yarn autoclean --force"
    ],
    "test": "vendor/bin/codecept run"
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ]
    },
    "yii\\composer\\Installer::postInstall": {
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/masgeek/file-manager"
    }
  ]
}
